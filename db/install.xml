<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/catdup/db" VERSION="20161008" COMMENT="XMLDB file for Moodle local/catdup"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_catdup_tasks" COMMENT="Table of tasks">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="origin" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="destination" TYPE="char" LENGTH="200" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="state" TYPE="number" LENGTH="1" NOTNULL="true" DEFAULT="1" SEQUENCE="false" COMMENT="1 - in queue, 2 - in work, 3 - ready"/>
        <FIELD NAME="timecreated" TYPE="char" LENGTH="13" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="char" LENGTH="13" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
